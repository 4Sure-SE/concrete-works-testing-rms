        "@radix-ui/react-tooltip": "^1.1.8",
        "@react-pdf/renderer": "^4.3.0",
        "@supabase/ssr": "^0.6.1",
        "@supabase/supabase-js": "^2.49.4",
        "@t3-oss/env-nextjs": "^0.10.1",
        "@types/react-day-picker": "^5.2.1",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cookie": "^1.0.2",
        "date-fns": "^3.6.0",
        "iconv-lite": "^0.6.3",
        "lucide-react": "^0.483.0",
        "next": "^15.2.3",
