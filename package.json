        "react": "19.1.0",
        "react-day-picker": "^9.6.7",
        "react-dom": "19.1.0",
        "react-dropzone": "^14.3.8",
        "react-hook-form": "^7.55.0",
        "server-only": "^0.0.1",
        "sonner": "^2.0.2",
        "tailwind-merge": "^3.0.2",
        "tailwindcss-animate": "^1.0.7",
        "tough-cookie": "^5.1.2",
        "tw-animate-css": "^1.2.4",
        "use-debounce": "^10.0.4",
        "zod": "^3.24.2"
