    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint",
            "prettier --write"
        ],
        "*.css": [
            "prettier --write"
        ],
        "*.{json,md}": [
            "prettier --write"
        ]
    },
    "packageManager": "npm@11.1.0",
    "prisma": {
        "schema": "src/server/db/schema.prisma",
        "seed": "tsx src/server/db/seed.ts"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "*",
        "@tailwindcss/oxide-linux-x64-gnu": "*",
